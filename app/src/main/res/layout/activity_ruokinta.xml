<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    tools:context=".Ruokinta">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="137dp"
        android:layout_height="142dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/chihuahua" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="137dp"
        android:layout_height="162dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/canine" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="274dp"
        android:layout_height="304dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/whitedog" />

    <EditText
        android:id="@+id/feedTime"
        android:layout_width="61dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:backgroundTint="#000000"
        android:ems="10"
        android:fontFamily="@font/share"
        android:inputType="time"
        android:text="17:01"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <EditText
        android:id="@+id/feedDate"
        android:layout_width="125dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:ems="10"
        android:fontFamily="@font/share"
        android:inputType="date"
        android:text="18/02/2020"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/feedTime"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/feedBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:height="55dp"
        android:fontFamily="@font/share"
        android:text="Ruoki"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/feedDate"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497" />

    <EditText
        android:id="@+id/feedFood"
        android:layout_width="218dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:fontFamily="@font/share"
        android:inputType="textPersonName"
        android:text="Golden Eagle"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedTime" />

    <EditText
        android:id="@+id/feedAmount"
        android:layout_width="83dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:fontFamily="@font/share"
        android:gravity="center"
        android:inputType="number"
        android:text="125g"
        android:textSize="24dp"
        app:layout_constraintStart_toEndOf="@+id/feedFood"
        app:layout_constraintTop_toBottomOf="@+id/feedBtn" />

    <TextView
        android:id="@+id/feedNotesHeadline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/share"
        android:text="Muistiinpanot"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/FeedNotes"
        app:layout_constraintStart_toStartOf="@+id/FeedNotes" />

    <EditText
        android:id="@+id/FeedNotes"
        android:layout_width="339dp"
        android:layout_height="162dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="16dp"
        android:background="@android:drawable/editbox_background_normal"
        android:ems="10"
        android:fontFamily="@font/share"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:singleLine="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nameDog1"
        android:layout_width="65dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/share"
        android:inputType="textMultiLine"
        android:text="Justi \n8V  4kg"
        android:textColor="#F6F6F6"
        android:textColorHighlight="#0500DF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3" />

    <TextView
        android:id="@+id/nameDog3"
        android:layout_width="65dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="@font/share"
        android:inputType="textMultiLine"
        android:text="Musti \n6V  10kg"
        android:textColor="#F6F6F6"
        android:textColorHighlight="#0500DF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView" />

    <TextView
        android:id="@+id/nameDog2"
        android:layout_width="65dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/share"
        android:inputType="textMultiLine"
        android:text="Kusti \n3V  18kg"
        android:textColor="#F6F6F6"
        android:textColorHighlight="#0500DF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2" />

</androidx.constraintlayout.widget.ConstraintLayout>